<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://smalldemo.chinacloudsites.cn</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>smalldemo.scm.chinacloudsites.cn:443</MSDeployServiceURL>
    <DeployIisAppPath>SmallDemo</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$SmallDemo</UserName>
    <_SavePWD>True</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="DataBaseEntities" Order="1" Enabled="False">
          <Destination Path="Data Source=61.152.104.247;Initial Catalog=testdb;User ID=developer;Password=EDTeam01!" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="SmallDemo.Models.Entities.DataBaseEntities, SmallDemo" Origin="Configuration" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="DefaultConnection" Order="2" Enabled="False">
          <Destination Path="Data Source=61.152.104.247;Initial Catalog=testdb;User ID=developer;Password=EDTeam01!" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="SmallDemo.Models.ApplicationDbContext, SmallDemo" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DataBaseEntities-Web.config Connection String">
      <ParameterValue>metadata=res://*/Models.Entities.Model1.csdl|res://*/Models.Entities.Model1.ssdl|res://*/Models.Entities.Model1.msl;provider=System.Data.SqlClient;provider connection string="Data Source=61.152.104.247;Initial Catalog=testdb;User ID=developer;Password=EDTeam01!"</ParameterValue>
    </MSDeployParameterValue>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DefaultConnection-Web.config Connection String">
      <ParameterValue>Data Source=61.152.104.247;Initial Catalog=testdb;User ID=developer;Password=EDTeam01!</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
